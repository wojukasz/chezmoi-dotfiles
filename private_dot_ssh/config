# Global Settings
ForwardAgent yes
#VisualHostKey yes
#HashKnownHosts no
#ConnectTimeout 2
IdentitiesOnly yes

Host gitlab.com
  Preferredauthentications publickey
  IdentityFile ~/.ssh/gitkey

Host github.com
  Preferredauthentications publickey
  IdentityFile ~/.ssh/gitkey
