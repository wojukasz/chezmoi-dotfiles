# SSH over Session Manager
host i-* mi-*
    ProxyCommand sh -c "aws ssm start-session --target %h --document-name AWS-StartSSHSession --parameters 'portNumber=%p'"

host gitlab.com
    Hostname gitlab.com
    User git
    Preferredauthentications publickey
    IdentityFile ~/.ssh/gitkey

host github.com
    Hostname github.com
    User git
    Preferredauthentications publickey
    IdentityFile ~/.ssh/gitkey
